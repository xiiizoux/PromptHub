# 后端API服务器配置
PORT=9010
# 前端服务器配置
FRONTEND_PORT=9011

# 传输类型配置 (stdio 用于本地运行, http用于远程模式)
TRANSPORT_TYPE=http

# 存储类型
STORAGE_TYPE=supabase

# API密钥配置 (用于MCP服务器认证)
# 系统级API密钥 - 用于服务器间通信 (应该使用独立的系统密钥，不是用户密钥)
API_KEY=development-default-key
# SERVER_KEY=system-level-server-key-here

# 安全配置（详细配置参考文件security-levels-config-guide.env）
SECURITY_LEVEL=balanced
RATE_LIMIT_ENABLED=true
AUTH_RATE_LIMIT_MAX=10

# 安全配置（详细配置参考文件security-levels-config-guide.env）
SECURITY_LEVEL=balanced
RATE_LIMIT_ENABLED=true
AUTH_RATE_LIMIT_MAX=10

# Supabase 配置 (必需)
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Supabase前端需要添加NEXT_PUBLIC_前缀的变量
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
NEXT_PUBLIC_SUPABASE_SERVICE_ROLE_KEY=

# JWT配置
JWT_SECRET=
JWT_EXPIRES_IN=3600

# Google OAuth 配置
NEXT_PUBLIC_GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# AI分析服务配置
OPENAI_API_KEY=
NEXT_PUBLIC_OPENAI_API_KEY=

# OpenAI兼容API配置 (支持自定义端点)
OPENAI_API_BASE_URL=
NEXT_PUBLIC_OPENAI_BASE_URL=
# 示例替代配置:
# OPENAI_API_BASE_URL=http://localhost:11434/v1  # Ollama
# OPENAI_API_BASE_URL=https://api.deepseek.com/v1  # DeepSeek
# OPENAI_API_BASE_URL=https://api.moonshot.cn/v1  # Moonshot
# OPENAI_API_BASE_URL=https://api.zhipuai.cn/api/paas/v4  # GLM

# 模型配置
AI_MODEL_FULL_ANALYSIS=
AI_MODEL_QUICK_TASKS=
NEXT_PUBLIC_AI_MODEL_FULL_ANALYSIS=
NEXT_PUBLIC_AI_MODEL_QUICK_TASKS=
# 本地模型示例:
# AI_MODEL_FULL_ANALYSIS=llama3:8b
# AI_MODEL_QUICK_TASKS=qwen2:7b

# 额外的环境变量配置
COMPOSE_BAKE=true
API_URL=http://localhost:9010
BACKEND_URL=http://localhost:9010
MCP_URL=http://localhost:9010
CORS_ORIGIN=*
NODE_ENV=development